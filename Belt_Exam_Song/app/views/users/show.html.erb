
<h2><%= @user.first_name%>'s Playlist</h2>

<table>
	<thead>
		<tr>
			<th>Artist</th>
			<th>Title</th>
			<th>Times Added</th>
		</tr>
	</thead>
	<tbody>
			<% @songs.each do |s| %>
			<tr>
				<td><%= s.artist%></td>
				<td><%= s.title%></td>
				<td><%= s.count%></td>
			</tr>
		<% end %>
	</tbody>

</table>
